                            groupPosition = registerTeamUI.readGroupPositionInput(scanner, tutorialGroups, program);
                            selectedGroup = tutorialGroups.get(groupPosition - 1);

                            teamPosition = registerTeamUI.readTeamPositionInput(scanner, teams, selectedGroup);

                            if (teamPosition != -1) {
                                selectedTeam = teams.get(teamPosition - 1);
                                ManageStudentTeam.listTeamStudents(selectedTeam);
                            }

                            break;

                        case 11:
                            System.out.println("Exiting...");
                            break;

                        default:
                            System.out.println("Functionality not implemented yet.");
                    }

                } while (subChoice != 10);
                break;

